class Cat:
  def __init__(self, name,name2):
    self.name = name
    self.name2 = name2
    

  def greeting(self):
    print("Hello my name is " + self.name +"!" + " I see you are also a cool fluffy kitty  " + self.name2 +" let's together purr at the human,so that they shall give us food!")

  def greeting2(self):
    print("Hello I am " + self.name2 + ". So nice to meet you,this sounds like a great idea!")
